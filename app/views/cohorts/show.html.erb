<section id="profile">
  <div class="row">
    <div class="col-lg-12 text-center">
      <h2>Alumni: <%= @cohort.title %></h2>
      <hr>
    </div>
  </div>
  <div class="row">
    <% @cohort.contacts.each do |contact| %>
      <%= render partial: "contacts/contact", :locals => { :contact => contact } %>
    <% end %> <!-- @cohort.contacts loop end -->
  </div><!-- row -->
</section>
