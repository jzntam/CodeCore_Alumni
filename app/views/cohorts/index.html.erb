<h1>Cohorts#index</h1>
<p>Find me in app/views/cohorts/index.html.erb</p>

<%= link_to "New Group", new_cohort_path %>
<br>
<div>
  <% @cohorts.each do |cohort| %>
    <%= cohort.title %>
    <%= cohort.details %>
    <%= link_to "Edit", edit_cohort_path(cohort) %>
    <%= link_to "Delete", cohort_path(cohort), method: :delete %>
  <% end %>
</div>